{
  "name": "hello-world",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "ng serve --aot --proxy-conf proxy.conf.json --port 4200",
    "server": "nodemon api/index.js --watch api --exec babel-node",
    "build": "ng build --aot --prod",
    "test": "npm run build",
    "schema:download": "apollo-codegen download-schema http://localhost:4300/graphql --output schema.json",
    "schema:generate": "apollo-codegen generate src/graphql/*.graphql --schema schema.json --target typescript --output src/graphql/schema.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/platform-server": "^4.1.3",
    "@angular/router": "^4.1.3",
    "apollo-angular": "^0.13.0",
    "apollo-client": "^1.2.2",
    "core-js": "^2.4.1",
    "graphql-tag": "^2.1.0",
    "rxjs": "^5.4.0",
    "ts-helpers": "~1.1.2",
    "zone.js": "~0.8.11"
  },
  "devDependencies": {
    "@angular/cli": "1.0.4",
    "@angular/compiler-cli": "^4.1.3",
    "@types/graphql": "^0.9.1",
    "apollo-codegen": "^0.10.13",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "casual": "^1.5.12",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.4.0",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "graphql": "^0.9.6",
    "graphql-server-express": "^0.7.2",
    "graphql-tools": "~0.11.0",
    "nodemon": "^1.11.0",
    "ts-node": "3.0.4",
    "tslint": "^5.3.2",
    "typescript": "~2.3.3"
  }
}
